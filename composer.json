{
  "name": "discordier/justtextwidgets",
  "description": "Hidden fields with text values and explanation texts",
  "keywords": [
    "contao",
    "widget",
    "text"
  ],
  "type": "contao-module",
  "homepage": "https://www.cyberspectrum.de",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "info@contao-community-alliance.de",
    "issues": "https://github.com/discordier/justtextwidgets/issues",
    "irc": "irc://irc.freenode.org/contao.dev",
    "source": "https://github.com/discordier/justtextwidgets"
  },
  "require": {
    "php": "^5.6 | ^7.0",
    "contao/core-bundle": "^3.5 | ^4.4",
    "contao-community-alliance/composer-plugin": "^2.4 | ^3.0"
  },
  "require-dev": {
    "phpcq/all-tasks": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Discordier\\JustTextWidgetsBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Discordier\\JustTextWidgetsBundle\\Test\\": "tests"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/justtextwidgets"
      }
    },
    "branch-alias": {
      "dev-support/1.1": "1.1.x-dev"
    }
  }
}
